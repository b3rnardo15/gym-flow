{
  "name": "Gym Flow",
  "short_name": "GymFlow",
  "description": "Transforme seu treino com Gym Flow!",
  "theme_color": "#1b376a",
  "background_color": "#265ca2",
  "start_url": "/index.html",
  "display": "standalone",
  "id": "/gym-flow",
  "launch_handler": {
    "action": "navigate",
    "url": "/home"
  },
  "orientation": "portrait",
  "screenshots": [
    {
      "src": "screenshots/screen1.png",
      "sizes": "320x240",
      "type": "image/png"
    },
    {
      "src": "screenshots/screen2.png",
      "sizes": "640x480",
      "type": "image/png"
    }
  ],
  "icons": [
    {
      "src": "icons/android-launchericon-48-48.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "icons/android-launchericon-72-72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "icons/android-launchericon-96-96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "icons/android-launchericon-144-144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "icons/android-launchericon-192-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/android-launchericon-512-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "categories": [
    "fitness",
    "sports"
  ],
  
  // Campos Opcionais
  "text_direction": "ltr",  
  "iarc_rating_id": "12345", 
  "lang": "pt-BR", 
  
  "prefer_related_applications": true,  
  "related_applications": [
    {
      "platform": "web",
      "url": "https://www.gymflow.com"
    },
    {
      "platform": "android",
      "url": "https://play.google.com/store/apps/details?id=com.gymflow"
    }
  ],  
  "scope": "/home",  
  "scope_extensions": [  
    "/dashboard",
    "/profile"
  ]
}
